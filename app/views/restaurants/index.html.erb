<h1>All restaurants</h1>
<ul class="list-group">
<% @restaurants.each do |restaurant| %>
<li class="list-group-item"><%= link_to restaurant.name, restaurant_path(restaurant) %> - <%= restaurant.address %></li>
</ul>
<%end  %>
<%= link_to "Add restaurant", new_restaurant_path, class: "btn btn-primary mt-3" %>
